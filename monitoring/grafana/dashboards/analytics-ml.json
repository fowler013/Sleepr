{
  "dashboard": {
    "id": null,
    "title": "Analytics & ML Performance",
    "tags": ["sleepr", "analytics", "ml", "python"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Model Training Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "(ml_model_training_success_total / (ml_model_training_success_total + ml_model_training_failures_total)) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Prediction Accuracy",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(ml_model_accuracy_score)",
            "legendFormat": "Average Accuracy"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.85}
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Analysis Processing Time",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(analysis_processing_duration_seconds)",
            "legendFormat": "Avg Processing Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 30},
                {"color": "red", "value": 60}
              ]
            },
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Pending Analysis Jobs",
        "type": "stat",
        "targets": [
          {
            "expr": "analytics_pending_jobs",
            "legendFormat": "Pending Jobs"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Model Performance Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "ml_model_accuracy_score",
            "legendFormat": "{{model_type}} Accuracy"
          },
          {
            "expr": "ml_model_precision_score",
            "legendFormat": "{{model_type}} Precision"
          },
          {
            "expr": "ml_model_recall_score",
            "legendFormat": "{{model_type}} Recall"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Analysis Queue Length",
        "type": "timeseries",
        "targets": [
          {
            "expr": "analytics_queue_length",
            "legendFormat": "Queue Length"
          },
          {
            "expr": "rate(analytics_jobs_processed_total[5m]) * 60",
            "legendFormat": "Processing Rate (per minute)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Data Quality Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "data_quality_score",
            "legendFormat": "Overall Quality Score"
          },
          {
            "expr": "data_completeness_ratio",
            "legendFormat": "Data Completeness"
          },
          {
            "expr": "data_freshness_hours",
            "legendFormat": "Data Freshness (hours)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Sleeper API Integration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(sleeper_api_requests_total[5m])",
            "legendFormat": "Request Rate"
          },
          {
            "expr": "rate(sleeper_api_errors_total[5m])",
            "legendFormat": "Error Rate"
          },
          {
            "expr": "sleeper_api_rate_limit_remaining",
            "legendFormat": "Rate Limit Remaining"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "Dynasty Analysis Results",
        "type": "table",
        "targets": [
          {
            "expr": "dynasty_player_value_predictions",
            "legendFormat": "Player Value Predictions",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            }
          },
          "overrides": []
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      }
    ],
    "time": {"from": "now-6h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "1m"
  }
}
